	background-image: url(../images/menu.png);
	position:relative;
	width: 100%;
	height:35px;
}
#slider{
	position:relative;
	background-image: url(../images/slider.png);
	width:950px;
	height:293px;
}
#content-top {
	position:relative;
	background-image:url(../images/content-top.png);
	width:990px;
	height:289px;
}
#content-mid {
	position:relative;
	background-image:url(../images/content-mid.png);
	width:990px;
	height:391px;
}
#content-bottom {
	position:relative;
	background-image:url(../images/content-bottom.png);
	width:990px;
}
#footer {
	position:relative;
	/*background-image:url(../images/footer.png);*/
	width:990px;
	height:30px;
	background-color: #4040F6;
	border-radius: 6px;
	color: #4040F6;
}
#copyright-text {
	position:absolute;
	left: 659px;
	top: 6px;
	font-size: 10px;
	width: 322px;
	overflow: auto;
	height: 21px;
}
.shadow-bottom-right {
	background: url(../images/image-shadow.png) bottom right no-repeat;
	padding: 0 7px 7px 0;
	float: left;
}
.shadow-bottom-right img {
	margin: 0 !important;
	display: block;
	padding: 8px;
	background: #ffffff;
	border: solid 1px #bcbcbc;
}

