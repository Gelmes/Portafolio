/*Top Stufff********************************************************/
body {
	/*background-image:url(../images/bg.png);*/
	padding: 0;
	text-align:left;
	margin: 0 auto;
	font-size:14px;
	font-family: 'Open Sans', sans-serif;
	background-repeat:repeat-x;
	background-color:#fff;
	/*background-color:#7373F8;*/
}
#wrapper {
	position:relative;
	width:950px;
	margin: 0px auto;
}
/*Header Stuff******************************************************/
#header {
	position:relative;
	background-image:url(../images/header.png);
	background-repeat:no-repeat;
	height:223px;
	width: 990px;
}
#call-us{
	position:absolute;
	top: 7px;
	left: 299px;
}
#logo{
	position:absolute;
	top: -5px;
	left: 18px;
	color: #F90;
	font-size: 60px;
	width: 446px;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
}
#phone{
	position:absolute;
	top: 13px;
	left: 551px;
	width: 232px;
	height: 31px;
}
#license{
	position:absolute;
	top: 194px;
	left: 438px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	font-weight: normal;
	color: #666;
}
#facebook{
	position:absolute;
	top: 191px;
	left: 76px;
